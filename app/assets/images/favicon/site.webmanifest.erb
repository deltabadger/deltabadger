{
  "name": "Deltabadger",
  "start_url": "/",
  "icons": [
    {
      "src": "<%= asset_path 'favicon/web-app-manifest-192x192.png' %>",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "<%= asset_path 'favicon/web-app-manifest-512x512.png' %>",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "<%= asset_path 'screenshots/desktop.png' %>",
      "type": "image/png",
      "sizes": "1280x720",
      "form_factor": "wide"
    },
    {
      "src": "<%= asset_path 'screenshots/mobile.png' %>",
      "type": "image/png",
      "sizes": "640x1136",
      "form_factor": "narrow"
    }
  ],
  "theme_color": "#001933",
  "background_color": "#ffffff",
  "display": "standalone"
}