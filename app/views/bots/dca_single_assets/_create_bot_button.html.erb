<%= form_with model: @bot, url: bots_dca_single_assets_path(@bot), method: :post, data: { action: "turbo:submit-end->modal--base#submitEnd" }, id: "create-bot-button" do |f| %>
  <%= f.submit t('button.start'), class: 'button button--large button--success', style: "min-width: 30rem", disabled: !@bot.valid? %>
<% end %>