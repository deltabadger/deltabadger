<%= turbo_frame_tag page_frame_id do %>
  <% assets.each do |id, symbol, name, color, exchanges| %>
    <%= render partial: "bots/asset_search_result", locals: { bot: bot, asset_field: asset_field, id: id, symbol: symbol, name: name, color: color, exchanges: exchanges } %>
  <% end %>
  <% if next_page_url %>
    <%= turbo_frame_tag next_page_frame_id, src: next_page_url, loading: :lazy %>
  <% end %>
<% end %>
